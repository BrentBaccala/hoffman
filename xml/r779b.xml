<?xml version="1.0"?>
<!DOCTYPE tablebase SYSTEM "http://www.freesoft.org/software/hoffman/tablebase.dtd">
<!-- Futurebase has two semilegal groups that span a local semilegal group -->
<!-- the two semilegal groups are the a-d files and the e-h files -->
<!-- three futurebases: one with one queen in each group, two with both queens in one group -->
<tablebase>
  <prune-enable color="white" type="discard"/>
  <dtm/>
  <piece color="white" type="king"/>
  <piece color="black" type="king"/>
  <piece color="white" type="queen" location="a1 a2 a3 a4 a5 a6 a7 a8 b1 b2 b3 b4 b5 b6 b7 b8 c1 c2 c3 c4 c5 c6 c7 c8 d1 d2 d3 d4 d5 d6 d7 d8"/>
  <piece color="white" type="queen" location="e1 e2 e3 e4 e5 e6 e7 e8 f1 f2 f3 f4 f5 f6 f7 f8 g1 g2 g3 g4 g5 g6 g7 g8 h1 h2 h3 h4 h5 h6 h7 h8"/>
  <futurebase filename="kqk.htb"/>
  <prune color="white" type="discard" move="Q*"/>
  <output filename="r779b.htb"/>
</tablebase>
